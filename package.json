{
  "name": "react-hn",
  "version": "1.6.1",
  "description": "React-powered frontend for Hacker News using its Firebase API",
  "author": "Jonny Buchanan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/insin/react-hn.git"
  },
  "scripts": {
    "webpack:serve": "nwb serve-react-app --babel.stage=2",
    "webpack:build": "nwb build-react-app --babel.stage=2",
    "parcel:serve": "parcel ./src/App.js",
    "parcel:build": "parcel build ./src/App.js",
    "clean": "rm -rf .cache dist # Run this if you want a clean build without cache"
  },
  "main": "server.js",
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.0",
    "ejs": "^2.4.1",
    "events": "1.1.1",
    "express": "^4.13.4",
    "firebase": "3.4.1",
    "history": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "nwb": "0.12.2",
    "object-assign": "^4.1.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "2.8.1",
    "react-router-scroll": "^0.3.2",
    "react-timeago": "3.1.3",
    "reactfire": "1.0.0",
    "scroll-behavior": "0.8.1",
    "setimmediate": "1.0.5",
    "url-parse": "^1.1.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.7.1"
  }
}
